<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="requestFriendViewModel"
            type="com.example.websocketclient.viewmodels.RequestFriendViewModel" />
    </data>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="25sp"
            android:text="@{requestFriendViewModel.dialogTextView}">
        </TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="확인" >
                <!--android:onClick="@{() -> requestFriendViewModel.duplicateAcceptButtonClicked()}"-->
            </Button>
            <Button
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="취소" >
                <!--android:onClick="@{() -> requestFriendViewModel.duplicateDenyButtonClicked()}"-->
            </Button>
        </LinearLayout>

    </LinearLayout>
</layout>